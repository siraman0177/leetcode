# brute force

class Solution {
public:
    ListNode* middleNode(ListNode* head) {
        
        ListNode*temp=head;
        int i=0;
        while (temp!=NULL)
         {i=i+1;
         temp=temp->next;
         }
         temp=head;

        for (int j=0;j<i/2;j++)
        {
            temp=temp->next;
        }
        return temp;
    }
};
