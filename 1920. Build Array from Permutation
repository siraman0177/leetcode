#python code

class Solution:
    def buildArray(self, nums: List[int]) -> List[int]:
        ans=[0]*len(nums)
        for i in range (len(nums)):
            arr=nums[i]
            ans[i]=nums[arr]
    
        return ans

#this works too

class Solution:
    def buildArray(self, nums: List[int]) -> List[int]:
        ans=[]
        for i in range (len(nums)):  
            ans.append(nums[nums[i]])
        return ans
