class Solution {
public:
    vector<vector<int>>pmt(vector<int>& nums, int idx,vector<vector<int>>ans)
    {
        if (idx==nums.size())
        {
            ans.push_back(nums);
            return ans;
        }

        for (int i=idx;i<nums.size();i++)
        {
            swap(nums[i],nums[idx]);
            ans=pmt(nums,idx+1,ans);
            swap(nums[i],nums[idx]);
        }
        return ans;
        
    }

    vector<vector<int>> permute(vector<int>& nums) {
        vector<vector<int>>ans;

        ans=pmt(nums,0,ans);
        return ans;
    }
};
